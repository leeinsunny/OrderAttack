<?xml version="1.0" encoding="utf-8"?>

                                                              
                                                        
<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".game.kiosk.Step1.home_beforeorder" android:tag="layout/activity_home_beforeorder_0" xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto">


    <ImageView
        android:id="@+id/home_beforeorder"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:src="@drawable/home_beforeorder"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="-202dp" />

    <ImageButton
        android:id="@+id/kiosk_burgertab"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@android:color/transparent"
        android:src="@drawable/burgertab"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.25" />

    <ImageView
        android:id="@+id/kpop_burgertab"
        android:layout_width="275dp"
        android:layout_height="70dp"
        android:layout_marginTop="52dp"
        android:src="@drawable/kpop_3"
        app:layout_constraintBottom_toBottomOf="@+id/home_beforeorder"
        app:layout_constraintEnd_toEndOf="@+id/home_beforeorder"
        app:layout_constraintHorizontal_bias="0.117"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/kiosk_burgertab"
        app:layout_constraintVertical_bias="0.227" />

    <ImageView
        android:id="@+id/kipoint"
        android:layout_width="51dp"
        android:layout_height="51dp"
        android:layout_marginStart="23dp"
        android:layout_marginBottom="92dp"
        android:src="@drawable/realpinkkiopoint"
        app:layout_constraintBottom_toTopOf="@+id/kpop_burgertab"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.085"
        app:layout_constraintStart_toStartOf="@+id/kiosk_burgertab"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.871" />

</androidx.constraintlayout.widget.ConstraintLayout>         